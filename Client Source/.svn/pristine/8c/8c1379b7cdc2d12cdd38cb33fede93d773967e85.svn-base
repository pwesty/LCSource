			else if(nPKState >= -19000 && nPKState < -6000 && buseblindchao)
				nColIndex = 18;
			else if(nPKState >= -32000 && nPKState < -19000 && buseblindchao)
				nColIndex = 19;
			else nColIndex = 9;
#else
