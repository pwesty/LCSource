	m_pixMinJ = pdp->dp_MinJ;
	m_pixMaxI = pdp->dp_MaxI;
	m_pixMaxJ = pdp->dp_MaxJ;

	for( int iUI = UI_TYPE_START; iUI < UI_TYPE_END; iUI++ )
